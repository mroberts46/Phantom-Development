<div class="bs-header" id="content">
	<div class="container">
		<div id="carousel" class="carousel slide" data-ride="#carousel">
			<ol class="carousel-indicators">
				<li data-target="#carousel" data-slide-to="0" class="active"></li>
				<li data-target="#carousel" data-slide-to="1"></li>
				<li data-target="#carousel" data-slide-to="2"></li>
				<li data-target="#carousel" data-slide-to="3"></li>
				<li data-target="#carousel" data-slide-to="4"></li>
			</ol>
			<div class="carousel-inner" role="listbox">
                    <div class="item active">
                         <img alt="First Slide" src="http://lorempixel.com/800/300/sports" />
                         <div class="container">
                              <div class="carousel-caption">
                                   <h1>{{title}}</h1>
	                              <p>{{ default lead description }}</p>
                                   <p><a class="btn btn-lg btn-primary" href="#" role="button">Read More</a> </p>
                              </div>
                         </div>
                    </div>
				<div class="item">
					<img alt="Second Slide" src="http://lorempixel.com/800/300/sports" />
					<div class="container">
						<div class="carousel-caption">
							<h1>{{title}}</h1>
							<p>{{ default lead description }}</p>
							<p><a class="btn btn-lg btn-primary" href="#" role="button">Read More</a> </p>
						</div>
					</div>
				</div>
				<div class="item">
					<img alt="Third Slide" src="http://lorempixel.com/800/300/sports" />
					<div class="container">
						<div class="carousel-caption">
							<h1>{{title}}</h1>
							<p>{{ default lead description }}</p>
							<p><a class="btn btn-lg btn-primary" href="#" role="button">Read More</a> </p>
						</div>
					</div>
				</div>
				<div class="item">
					<img alt="Fourth Slide" src="http://lorempixel.com/800/300/sports" />
					<div class="container">
						<div class="carousel-caption">
							<h1>{{title}}</h1>
							<p>{{ default lead description }}</p>
							<p><a class="btn btn-lg btn-primary" href="#" role="button">Read More</a> </p>
						</div>
					</div>
				</div>
				<div class="item">
					<img alt="Fifth Slide" src="http://lorempixel.com/800/300/sports" />
					<div class="container">
						<div class="carousel-caption">
							<h1>{{title}}</h1>
							<p>{{ default lead description }}</p>
							<p><a class="btn btn-lg btn-primary" href="#" role="button">Read More</a> </p>
						</div>
					</div>
				</div>
			</div>
			<a class="left carousel-control" href="#carousel" role="button" data-slide="prev">
				<span class="glyphicon glyphicon-chevron-left"></span>
				<span class="sr-only">Previous</span>
			</a>
			<a class="right carousel-control" href="#carousel" role="button" data-slide="next">
				<span class="glyphicon glyphicon-chevron-right"></span>
				<span class="sr-only">Next</span>
			</a>
		</div>
	</div>
</div>